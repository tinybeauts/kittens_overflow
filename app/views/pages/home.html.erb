<% if is_logged_in? %>
  <%= "Welcome, #{current_user.username}"%>
<% else %>
  <%= render "shared/login_form" %>
  <%= render "shared/user_signup_form" %>
<% end %>

<% @images.each do |image| %>
  <%= render 'shared/vote', :obj => image %>
  <%= link_to(image_tag(image.source_url), image_path(image.id)) %>
<% end %>

